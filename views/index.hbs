                                        <select class="u-full-width" type="text" placeholder='first name'>
                                            <option value="">Wedding</option>
                                            <option value="">Bar/Bat Mitzvah</option>
                                            <option value="">Sweet Sixteen</option>
                                            <option value="">Anniversary</option>
                                            <option value="">Elopement</option>
                                            <option value="">Engagement</option>
                                        </select>
                                    </div>
                                </div>
                                <div class="four columns">
                                    <div class="form-input">
                                        <label for="">Date</label>
                                        <input class="u-full-width" type="date" placeholder='first name'/>
                                    </div>
                                </div>
                                <div class="four columns">
                                    <div class="form-input">
                                        <label for="">Location</label>
                                        <input class="u-full-width" type="text" placeholder='Event Location'/>
                                    </div>
                                </div>
                            </div>
                            <div class="twelve columns">
                                                                <div class='form-input'>
